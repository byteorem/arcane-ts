{
  "name": "wow-arcane-ts",
  "version": "0.1.1",
  "description": "TypeScript library for Blizzard's World of Warcraft API",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "bunchee",
    "check": "biome check .",
    "check:write": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "pnpm build && ava"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "wow",
    "blizzard",
    "armory",
    "api",
    "typescript"
  ],
  "author": "Byteorem",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "ava": "^6.4.1",
    "bunchee": "^6.6.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "ky": "^1.10.0",
    "slugify": "^1.6.6"
  },
  "ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--import=tsx/esm"
		],
		"workerThreads": false
	}
}